model likes {
  userId    String   @db.Uuid
  productId String
  products  products @relation(fields: [productId], references: [id], onDelete: Cascade)
  profiles  profiles @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@id([userId, productId])
  @@schema("public")
}

model products {
  id            String   @id(map: "Product_pkey")
  title         String
  description   String
  sizes         String?
  userId        String   @db.Uuid
  imageUrls     String[]
  price         Float
  type          String
  governorate   String
  contactNumber String?  @default("")
  instagramLink String?  @default("")
  likes         likes[]
  profiles      profiles @relation(fields: [userId], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "products_userId_fkey1")

  @@schema("public")
}

/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model profiles {
  id           String     @id @db.Uuid
  full_name    String?
  profile_url  String?
  bio          String?
  phone_number String?
  avatar       String?
  links        String[]
  likes        likes[]
  products     products[]

  @@schema("public")
}

enum aal_level {
  aal1
  aal2
  aal3

  @@schema("auth")
}

enum code_challenge_method {
  s256
  plain

  @@schema("auth")
}

enum factor_status {
  unverified
  verified

  @@schema("auth")
}

enum factor_type {
  totp
  webauthn
  phone

  @@schema("auth")
}

enum one_time_token_type {
  confirmation_token
  reauthentication_token
  recovery_token
  email_change_token_new
  email_change_token_current
  phone_change_token

  @@schema("auth")
}
